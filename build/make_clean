#!/bin/bash
set -ex

# Load variables
#
# Load all variables used in the build process.
for vars in variable*; do
   source $vars
done

# Execution
#
# Executing the script.
IMG="CONTAINER_IMAGE.var"
if [ -f "$IMG" ]
then
    CONTAINER_IMAGE=$(cat $IMG)
    docker rmi ${CONTAINER_IMAGE}
else
	echo "$IMG not found."
fi